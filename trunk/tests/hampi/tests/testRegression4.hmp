// automatically generated by ../../../YaccHampi/yparse.py
// with some small hand-tweaks
//   - added spaces between RHS rules
//   - created single-letter vars.
//   - added optional trailing space

// 6 works well, 7 works well, 8 is too big (runs out of memory!)
var v : 20;

cfg VAR := ['a'-'z'];

cfg program := expr
  | expr trailing_spaces;

// in order to overcome the fact that Hampi generates fixed-sized strings
cfg trailing_spaces := " "
    | trailing_spaces " ";

cfg expr := function
    | "(" expr ")"
    | "!" expr
    | expr " & " expr
    | expr " | " expr
    | expr " > " expr
    | expr " = " expr
    | expr " ^ " expr
    | quantvar " " expr ;

cfg quantvar := "@" VAR
    | "$" VAR ;

cfg function := VAR
    | VAR "(" list ")" ;

cfg list := function
    | function "," flist ;

cfg flist := list ;

// Hampi can only deal with bounded CFGs
reg Pbound := fix(program, 20);
assert v in Pbound;
