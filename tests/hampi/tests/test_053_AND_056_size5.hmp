/*CFG Analyzer, version 03/12/2007

A word in the intersection of L(G1) ... L(G2) is, e.g., "aaaaa" of length 5
*/
var hampiStringVar : 5;
cfg A0 :=  A0 "b" | "a" A0 | "b" | "a" ;
reg limit0 := fix(A0, 5);
assert hampiStringVar in limit0;
cfg S1 :=  B1 | A1 ;
cfg A1 :=  "a" | "a" A1 ;
cfg B1 :=  "a" | B1 "a" ;
reg limit1 := fix(S1, 5);
assert hampiStringVar in limit1;
